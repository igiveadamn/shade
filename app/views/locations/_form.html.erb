<p>
	<%= f.label :name %><br />
  <%= f.text_field :name %>
</p>
<p>
	<%= f.label :address %><br />
  <%= f.text_area :address, :rows => 4 %>
</p>
<p>
	<%= f.label :contact %><br />
  <%= f.text_field :contact %>&nbsp;&nbsp;<%= f.label :cell %><%= f.text_field :cell, :size => 15 %>
</p>
<p>
	<%= f.label :alt_contact %><br />
  <%= f.text_field :alt_contact %>&nbsp;&nbsp;<%= f.label :alt_cell %><%= f.text_field :alt_cell, :size => 15 %>
</p>
<p>
	<%= f.label :comments %><br />
  <%= f.text_area :comments, :rows => 3 %>
</p>
<p>
	<%= f.label :capacity %><br />
  <%= f.text_field :capacity, :size => 4, :maxlength => 4 %> people
</p>
